{
  "name": "a_design-components",
  "version": "1.0.2",
  "description": "React component for a UI design. Based on a_design.",
  "main": "build/index.js",
  "scripts": {
    "pretty": "a_codestyle -i config,tmp,build",
    "commit": "git-cz",
    "build-styl": "stylus-extra src -o src",
    "watch-styl": "stylus-extra src -w",
    "start": "node scripts/start.js",
    "prebuild": "npm run build-styl",
    "build": "node scripts/build.js;",
    "pretest": "npm run build-styl",
    "test": "CI=true node scripts/test.js",
    "test-coverage": "node scripts/test.js --coverage",
    "release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty"
    }
  },
  "homepage": "https://github.com/Alexgalinier/a_design-components",
  "keywords": [
    "a_design",
    "React",
    "Components",
    "UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Alexgalinier/a_design-components.git"
  },
  "bugs": {
    "url": "https://github.com/Alexgalinier/a_design-components/issues"
  },
  "author": "Alexgalinier",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "a_design": "^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "a_codestyle": "^3.0.1",
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.2",
    "commitizen": "^2.9.6",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "5.0.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "3.8.2",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "5.1.x",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "jest": "22.4.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "semantic-release": "^15.1.4",
    "style-loader": "0.20.3",
    "stylus-extra": "^1.0.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "3.8.x",
    "webpack-dev-server": "3.1.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "greenkeeper": {
    "ignore": [
      "webpack",
      "eslint-plugin-jsx-a11y"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
